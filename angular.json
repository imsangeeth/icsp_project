{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "icsp": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/icsp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "src/styles.css",
              "src/style/style.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/style/spinners.css",
              "src/style/animate.css",
              "src/style/icons/font-awesome/css/font-awesome.min.css",
              "src/style/icons/simple-line-icons/css/simple-line-icons.css",
              "src/style/icons/weather-icons/css/weather-icons.min.css",
              "src/style/icons/linea-icons/linea.css",
              "src/style/icons/themify-icons/themify-icons.css",
              "src/style/icons/flag-icon-css/flag-icon.min.css",
              "src/style/icons/material-design-iconic-font/css/materialdesignicons.min.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "style/awl.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "icsp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "icsp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "icsp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "src/styles.css",
              "src/style/style.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/style/spinners.css",
              "src/style/animate.css",
              "src/style/icons/font-awesome/css/font-awesome.min.css",
              "src/style/icons/simple-line-icons/css/simple-line-icons.css",
              "src/style/icons/weather-icons/css/weather-icons.min.css",
              "src/style/icons/linea-icons/linea.css",
              "src/style/icons/themify-icons/themify-icons.css",
              "src/style/icons/flag-icon-css/flag-icon.min.css",
              "src/style/icons/material-design-iconic-font/css/materialdesignicons.min.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/jquery/dist/jquery.js",
              "style/awl.min.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "icsp-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "icsp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "icsp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "icsp"
}